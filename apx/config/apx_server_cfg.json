{
   "server": {
      "apx-cache-enabled": false,
      "apx-cache-path": "",
      "shutdown-timer": 0,
      "max-num-events": 200
   },
   "extension": {
      "socket-server": {
         "extension-enabled": true,
         "tcp-port": 5000,
         "tcp-tag": "tcp",
         "unix-file": "/tmp/apx_server.socket",
         "unix-tag": "unix"
	   },
	  "textlog": {
	     "extension-enabled": true,
	     "file-enabled": true,
	     "file-path": "",
	     "syslog-enabled": false	     
	  },
      "command": {
         "extension-enabled": true,
         "connection-tag": "tcp"
      }
   }
}
